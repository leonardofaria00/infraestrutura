{
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[php]": {
        "editor.defaultFormatter": "vscode.php-language-features"
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "emmet.syntaxProfiles": {
        "javascript": "jsx"
    },
    "files.autoSave": "afterDelay",
    "workbench.iconTheme": "vscode-icons",
    "git.confirmSync": false,
    "debug.console.fontFamily": "default",
    "editor.fontFamily": "'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.lineHeight": 0,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "prettier.singleQuote": true,
    "editor.formatOnPaste": true,
    "diffEditor.ignoreTrimWhitespace": true,
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.startupEditor": "newUntitledFile",
    "git.autofetch": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "docker.defaultRegistryPath": "leonardofaria00",
    "git.suggestSmartCommit": true,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "html.format.enable": true,
    "typescript.format.enable": true,
    "php.validate.enable": true,
    "php.validate.executablePath": "",
    "javascript.format.enable": true,
    "java.home": "C:\\Program Files\\Java\\jdk1.8.0_261",
    //"java.home": "/usr/lib/jvm/java-8-openjdk-amd64/jre/bin/java",
    "java.codeGeneration.generateComments": true,
    "java.dependency.autoRefresh": true,
    "java.autobuild.enabled": true,
    "java.format.enabled": true,
    "java.saveActions.organizeImports": true,
    "java.maven.downloadSources": false,
    "java.maven.updateSnapshots": true,
    "java.semanticHighlighting.enabled": true,
    "explorer.confirmDelete": true,
    "shellformat.effectLanguages": [
        "shellscript",
        "dotenv",
        "hosts",
        "jvmoptions",
        "ignore",
        "gitignore",
        "properties",
        "spring-boot-properties",
        "azcli"
    ],
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    }
}
